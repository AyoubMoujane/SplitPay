<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="500.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controllers.LogInController">
   <children>
      <Pane prefHeight="67.0" prefWidth="320.0">
         <children>
            <Label layoutX="127.0" layoutY="14.0" text="Log in">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane prefHeight="395.0" prefWidth="320.0">
         <children>
            <TextField fx:id="credential" layoutX="86.0" layoutY="78.0" promptText="email or phone" />
            <PasswordField fx:id="password" layoutX="86.0" layoutY="142.0" promptText="password" />
            <Button layoutX="128.0" layoutY="217.0" mnemonicParsing="false" onAction="#logIn" prefHeight="35.0" prefWidth="65.0" text="Log in" />
         </children></Pane>
      <Pane prefHeight="51.0" prefWidth="320.0">
         <children>
            <Button layoutX="53.0" layoutY="11.0" mnemonicParsing="false" onAction="#goToNormalUserSignUpView" text="Sign in" />
            <Button layoutX="172.0" layoutY="11.0" mnemonicParsing="false" onAction="#goToForgottenPasswordView" text="Forgotten password?" />
         </children></Pane>
   </children>
</VBox>
